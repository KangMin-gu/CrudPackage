<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="callSettersOnNulls" value="true" />
	</settings>
	
	<typeAliases>
		<typeAlias type="saas.crud.crm.email.dto.EmailDto" alias="emailDto" />
		<typeAlias type="saas.crud.crm.au.dto.UserDto" alias="userDto" />
		<typeAlias type="saas.crud.crm.au.dto.MemCompanyDto" alias="memCompanyDto" />
		<typeAlias type="saas.crud.crm.au.dto.MenuDto" alias="menuDto" />
	</typeAliases>

	<mappers>
		<mapper resource="DB/mssql/EmailMapper.xml" />
		<mapper resource="DB/mssql/UserMapper.xml" />
		<mapper resource="DB/mssql/MemCompanyMapper.xml" />
		<mapper resource="DB/mssql/AdMapper.xml" />
		<mapper resource="DB/mssql/MenuMapper.xml" />				
	</mappers>
	
</configuration>