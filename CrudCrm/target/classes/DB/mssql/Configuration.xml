<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="callSettersOnNulls" value="true" />
	</settings>
	
	<typeAliases>
		<typeAlias type="saas.crud.crm.email.dto.EmailDto" alias="emailDto" />
		<typeAlias type="saas.crud.crm.au.dto.UserDto" alias="userDto" />
		<typeAlias type="saas.crud.crm.au.dto.MemCompanyDto" alias="memCompanyDto" />
		<typeAlias type="saas.crud.crm.nt.dto.NoteDto" alias="noteDto" />
		<typeAlias type="saas.crud.crm.nt.dto.NoteCategoryDto" alias="noteCategoryDto" />
		<typeAlias type="saas.crud.crm.ce.FileDto" alias="fileDto" />
	</typeAliases>

	<mappers>
		<mapper resource="DB/mssql/EmailMapper.xml" />
		<mapper resource="DB/mssql/UserMapper.xml" />		
		<mapper resource="DB/mssql/NoteMapper.xml" />			
		<mapper resource="DB/mssql/CustMapper.xml" />
		<mapper resource="DB/mssql/CommonMapper.xml" />		

	</mappers>
	
</configuration>














