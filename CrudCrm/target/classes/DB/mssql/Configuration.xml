<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="callSettersOnNulls" value="true" />
	</settings>
	
	<typeAliases>
		<typeAlias type="saas.crud.crm.email.dto.EmailDto" alias="emailDto" />
		<typeAlias type="saas.crud.crm.au.dto.UserDto" alias="userDto" />
		<typeAlias type="saas.crud.crm.au.dto.CompanyDto" alias="CompanyDto" />
		<typeAlias type="saas.crud.crm.au.dto.MenuDto" alias="menuDto" />
		<typeAlias type="saas.crud.crm.au.dto.LicenseDto" alias="licenseDto" />
		<typeAlias type="saas.crud.crm.au.dto.CodeDto" alias="codeDto"/>
		<typeAlias type="saas.crud.crm.sv.dto.RcvDto" alias="rcvDto"/>
		<typeAlias type="saas.crud.crm.sv.dto.RactDto" alias="ractDto"/>
		<typeAlias type="saas.crud.crm.sv.dto.ConveyDto" alias="conveyDto"/>
	</typeAliases>

	<mappers>
		<mapper resource="DB/mssql/EmailMapper.xml" />
		<mapper resource="DB/mssql/UserMapper.xml" />
		<mapper resource="DB/mssql/CompanyMapper.xml" />
		<mapper resource="DB/mssql/AuMapper.xml" />
		<mapper resource="DB/mssql/MenuMapper.xml" />
		<mapper resource="DB/mssql/LicenseMapper.xml" />	
		<mapper resource="DB/mssql/CodeMapper.xml"/>
		<mapper resource="DB/mssql/SvMapper.xml"/>			
	</mappers>
	
</configuration>